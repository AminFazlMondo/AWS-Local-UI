{
  "plugins": [
    "simple-import-sort"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "react-app",
    "@mondopower/eslint-config-mondo"
  ],
  "rules": {
    "simple-import-sort/imports": "warn",
    "simple-import-sort/exports": "warn"
  },
  "overrides": [
    {
      "files": "./src/**/*.js",
      "rules": {
        "react/prop-types": "off",
        "filenames/match-regex": "off",
        "no-console": "warn"
      }
    }
  ]
}